  "repository": {
    "type": "git",
    "url": "https://github.com/chscit12/EZStore"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0"
  }
}
